{
  "name": "meticulous-ui",
  "productName": "meticulous-ui",
  "version": "1.17.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "private": true,
  "scripts": {
    "format": "prettier --config .prettierrc \"{src/**/,.github/**/,}*.{tsx,ts,md,js,css,html,json,yml}\" --write",
    "format:check": "prettier --config .prettierrc \"{src/**/,.github/**/,}*.{tsx,ts,md,js,css,html,json,yml}\" --check",
    "commit": "git-cz",
    "start": "export SERVER_URL=http://localhost:8080 && electron-forge start",
    "start-window": "set SERVER_URL=http://localhost:8080 && electron-forge start",
    "types": "tsc --noEmit",
    "package": "NODE_ENV=production electron-forge package",
    "make": "NODE_ENV=production electron-forge make",
    "publish": "electron-forge publish",
    "prepare": "husky",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MeticulousHome/meticulous-dial"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --config .prettierrc \"src/**/*.{js,tsx,ts}\" --check --ignore-unknown"
  },
  "keywords": [],
  "author": {
    "name": "salatielosorno",
    "email": "salatiel.linares@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-webpack": "^7.4.0",
    "@eslint/js": "^9.10.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "commitizen": "^4.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "electron": "^32.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-import-resolver-webpack": "^0.13.9",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.36.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "less-plugin-unique-directives": "^0.1.0",
    "lint-staged": "^15.2.10",
    "node-loader": "^2.0.0",
    "prettier": "^3.3.3",
    "schema-utils": "^4.2.0",
    "semantic-release": "^24.1.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
  },
  "dependencies": {
    "@meticulous-home/espresso-api": "^0.5.7",
    "@meticulous-home/espresso-profile": "^0.4.2",
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "electron-squirrel-startup": "^1.0.1",
    "fast-deep-equal": "^3.1.3",
    "lottie-web": "^5.12.2",
    "nanoevents": "^9.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-freeze": "^1.0.4",
    "react-redux": "^9.1.2",
    "socket.io-client": "^4.7.5",
    "styled-components": "^6.1.13",
    "swiper": "^11.1.14",
    "uuid": "^10.0.0"
  }
}
