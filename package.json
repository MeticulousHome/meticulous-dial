    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "electron-squirrel-startup": "^1.0.1",
    "fast-deep-equal": "^3.1.3",
    "lottie-web": "^5.12.2",
    "nanoevents": "^7.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-freeze": "^1.0.4",
    "react-redux": "8.0.5",
    "socket.io-client": "^4.7.5",
    "styled-components": "^6.1.13",
    "swiper": "^9.4.1",
    "uuid": "^9.0.1"
  }
}
