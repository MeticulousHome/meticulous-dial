{
  "name": "meticulous-ui",
  "productName": "meticulous-ui",
  "version": "1.1.0-beta.1",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "private": true,
  "scripts": {
    "format": "prettier --config .prettierrc \"src/**/*.{tsx,ts}\" --write",
    "commit": "git-cz",
    "start": "export SERVER_URL=http://localhost:8080 && electron-forge start",
    "start-window": "set SERVER_URL=http://localhost:8080 && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PrivSocial/meticulous-ui"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --config .prettierrc \"src/**/*.{js,tsx,ts}\" --check --ignore-unknown"
  },
  "keywords": [],
  "author": {
    "name": "salatielosorno",
    "email": "salatiel.linares@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@commitlint/cz-commitlint": "^17.4.1",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@electron-forge/plugin-webpack": "^6.0.5",
    "@electron-forge/publisher-s3": "6.0.5",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "commitizen": "^4.2.6",
    "css-loader": "^6.0.0",
    "electron": "22.0.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "^8.0.3",
    "inquirer": "^8.2.5",
    "lint-staged": "^13.1.0",
    "node-loader": "^2.0.0",
    "prettier": "2.8.2",
    "semantic-release": "^20.0.2",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "8.0.5",
    "socket.io-client": "^4.5.4",
    "swiper": "^9.0.2"
  }
}
