{
  "name": "meticulous-ui",
  "productName": "meticulous-ui",
  "version": "1.13.2",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "private": true,
  "scripts": {
    "format": "prettier --config .prettierrc \"{src/**/,.github/**/,}*.{tsx,ts,md,js,css,html,json,yml}\" --write",
    "format:check": "prettier --config .prettierrc \"{src/**/,.github/**/,}*.{tsx,ts,md,js,css,html,json,yml}\" --check",
    "commit": "git-cz",
    "start": "export SERVER_URL=http://localhost:8080 && electron-forge start",
    "start-window": "set SERVER_URL=http://localhost:8080 && electron-forge start",
    "types": "tsc --noEmit",
    "package": "NODE_ENV=production electron-forge package",
    "make": "NODE_ENV=production electron-forge make",
    "publish": "electron-forge publish",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.tsx .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FFFuego/meticulous-dial"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --config .prettierrc \"src/**/*.{js,tsx,ts}\" --check --ignore-unknown"
  },
  "keywords": [],
  "author": {
    "name": "salatielosorno",
    "email": "salatiel.linares@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@commitlint/cz-commitlint": "^17.8.1",
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-webpack": "^6.4.2",
    "@electron-forge/publisher-s3": "6.0.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/addon-interactions": "^7.6.20",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-webpack5": "^7.6.20",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "commitizen": "^4.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "electron": "24.8.8",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-storybook": "^0.6.15",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.6",
    "less": "^4.2.0",
    "less-loader": "^11.1.4",
    "less-plugin-unique-directives": "^0.1.0",
    "lint-staged": "^13.3.0",
    "node-loader": "^2.0.0",
    "prettier": "2.8.2",
    "schema-utils": "^4.2.0",
    "semantic-release": "^20.1.3",
    "storybook": "^7.6.20",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@meticulous-home/espresso-api": "^0.5.3",
